# Copie para .env e ajuste os valores
METABASE_HOST=https://metabase.suaempresa.com

# Escolha UMA forma de autenticação
# 1) Sessão (usuário/senha):
METABASE_USERNAME=seu_usuario
METABASE_PASSWORD=sua_senha

# 2) Token pessoal (PAT):
# METABASE_API_KEY=xxxx-xxxx-xxxx-xxxx

# Onde salvar o CSV gerado
OUTPUT_DIR=/Users/seu_usuario/Exports/metabase

# Prefixo opcional para o nome do arquivo
FILE_PREFIX=relatorio
